{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start-react": "vite",
    "start-electron": "cross-env NODE_ENV=development nodemon --watch electron --ext js,cjs --exec \"electron ./electron/main.cjs\"",
    "dev": "concurrently \"bun run start-react\" \"bun run start-electron\""
  },
  "dependencies": {
    "bun-plugin-tailwind": "^0.0.15",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^38.2.0",
    "nodemon": "^3.1.10",
    "vite": "^7.1.7"
  }
}
